{
  "expo": {
    "name": "restaurant-order-management",
    "slug": "restaurant-order-management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android/res/mipmap-hdpi/ic_launcher_round.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/res/mipmap-hdpi/ic_launcher_round_foreground.png",
        "backgroundColor": "./assets/android/res/mipmap-hdpi/ic_launcher_round_background.png"
      },
      "package": "com.udk16.restaurantordermanagement"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/android/res/mipmap-hdpi/ic_launcher_round.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1cf63956-6cf7-4343-9514-8e00eac83524"
      }
    }
  }
}
